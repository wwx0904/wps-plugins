{
    "name": "ai-bidding-wps-installer",
    "version": "1.0.0",
    "description": "AI赢标助手WPS插件环境切换工具 - Tauri版本",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "tauri": "tauri",
        "tauri:dev": "tauri dev",
        "tauri:build": "tauri build",
        "start": "./start-dev.sh",
        "start:dev": "./start-dev.sh",
        "start:windows": "start-dev-windows.bat",
        "mac:build": "./build-mac/build.sh build",
        "windows:setup": "./build-windows/setup-and-build-windows.bat",
        "windows:build": "./build-windows/quick-build-windows.bat",
        "tauri:build-all": "tauri build --target universal-apple-darwin && tauri build --target x86_64-pc-windows-msvc && npm run copy-bundle",
        "copy-bundle": "mkdir -p dist && cp -r src-tauri/target/release/bundle/* dist/ && echo '✅ 构建产物已复制到 dist/ 目录'",
        "clean-dist": "rm -rf dist/",
        "clean-target": "rm -rf src-tauri/target/",
        "clean": "npm run clean-dist && npm run clean-target"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.1.0",
        "@tauri-apps/api": "^1.5.1",
        "element-plus": "^2.4.2",
        "vue": "^3.3.8"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^1.5.6",
        "@types/node": "^20.8.9",
        "@vitejs/plugin-vue": "^4.4.1",
        "sass": "^1.91.0",
        "unplugin-auto-import": "^0.16.7",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.5.0"
    }
}
